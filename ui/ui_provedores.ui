  <property name="styleSheet">
   <string notr="true">/* === Estilo general === */
QMainWindow {
    background-color: #f6f7fb;
    color: #333333;
    font-family: &quot;Segoe UI Variable&quot;, &quot;Roboto&quot;, &quot;Open Sans&quot;, sans-serif;
    font-size: 16px;
    font-weight: 300; /* Más delgada */
}

/* === Frames === */
QFrame {
    background-color: #ffffff;
    border-radius: 10px;
    border: 1px solid #dcdcdc;
}

/* Barra superior */
#frame_superior {
    background-color: #e8ecf1;
    border: none;
}

/* Panel lateral */
#frame_lateral {
    background-color: #f1f3f6;
    border-right: 1px solid #d1d1d1;
}

/* Área principal */
#frame_paginas {
    background-color: #ffffff;
}

/* === Botones === */
QPushButton {
    background-color: #f5f5f5;
    color: #333333;
    border: 1px solid #cccccc;
    border-radius: 6px;
    padding: 8px 14px;
    font-weight: 400;
    font-size: 15px;
    letter-spacing: 0.3px;
}

QPushButton:hover {
    background-color: #0078d7;
    color: #ffffff;
    border: 1px solid #0078d7;
}

QPushButton:pressed {
    background-color: #005a9e;
    color: #ffffff;
    border: 1px solid #005a9e;
}

/* Botón principal (Menú) */
#boton_menu {
    background-color: #0078d7;
    color: white;
    border: none;
    font-weight: 400;
}
#boton_menu:hover {
    background-color: #006cbe;
}
#boton_menu:pressed {
    background-color: #005a9e;
}

/* Botón salir */
#boton_salir {
    background-color: #e74c3c;
    color: white;
    border: none;
}
#boton_salir:hover {
    background-color: #c0392b;
}
#boton_salir:pressed {
    background-color: #a93226;
}

/* === Campos de texto === */
QLineEdit {
    background-color: #ffffff;
    color: #333333;
    border: 1px solid #cccccc;
    border-radius: 4px;
    padding: 6px 8px;
    font-size: 15px;
    font-weight: 300;
}

QLineEdit:focus {
    border: 1px solid #0078d7;
    background-color: #f9fcff;
}

/* === Labels === */
QLabel {
    color: #333333;
    font-weight: 300;
    font-size: 15px;
}

/* Label inferior o secundario */
#label {
    color: #666666;
    font-size: 13px;
    font-style: italic;
}

/* === Stacked Widget === */
QStackedWidget {
    background-color: #ffffff;
    border: 1px solid #dcdcdc;
    border-radius: 6px;
}

/* === Scrollbars === */
QScrollBar:vertical {
    border: none;
    background: #f0f0f0;
    width: 10px;
    border-radius: 4px;
}
QScrollBar::handle:vertical {
    background: #cccccc;
    min-height: 20px;
    border-radius: 4px;
}
QScrollBar::handle:vertical:hover {
    background: #0078d7;
}
QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0;
}

/* === Layouts === */
QVBoxLayout, QHBoxLayout {
    spacing: 10px;
}

/* === Botones laterales === */
#boton_agregar, #boton_buscar, #boton_actualizar, #boton_eliminar, #boton_consultar {
    background-color: #ffffff;
    border: 1px solid #cccccc;
    color: #333333;
    font-weight: 400;
    font-size: 15px;
}

#boton_agregar:hover, 
#boton_buscar:hover, 
#boton_actualizar:hover, 
#boton_eliminar:hover,
#boton_consultar:hover {
    background-color: #e8f0fe;
    border: 1px solid #0078d7;
    color: #0078d7;
}

#boton_agregar:pressed, 
#boton_buscar:pressed, 
#boton_actualizar:pressed, 
#boton_eliminar:pressed ,
#boton_consultar:pressed{
    background-color: #cce0ff;
}

/* === Tooltips === */
QToolTip {
    background-color: #ffffff;
    color: #333333;
    border: 1px solid #0078d7;
    padding: 6px;
    border-radius: 4px;
    font-size: 14px;
}</string>
